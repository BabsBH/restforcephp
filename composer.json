{
    "name": "eventfarm/restforcephp",
    "description": "PHP Salesforce REST",
    "license": "MIT",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jasonraimondi/oauth2-salesforce"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.6",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "stevenmaguire/oauth2-salesforce": "dev-php71"
    },
    "autoload": {
        "psr-4": {
            "EventFarm\\Restforce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventFarm\\Restforce\\Tests\\": "tests"
        }
    }
}
